print("=== Bubble Sort ===");
let arr = [5, 1, 4, 2, 8, 3, 2, 6, 7, 8, 1, 2, 3, 10, 12];
let n = len(arr);

for (let i = 0; i < n; i = i + 1) {
    for (let j = 0; j < n - 1 - i; j = j + 1) {
        if (arr[j] > arr[j + 1]) {
            let tmp = arr[j];
            arr[j] = arr[j + 1];
            arr[j + 1] = tmp;
        }
    }
}

printf("Sorted: [ ");
for (let i = 0; i < n; i = i + 1) {
    printf(str(arr[i]) + ", ");
}
printf("]");