print("=== Sieve of Eratosthenes up to N ===");
let N = 100;
let flags = [];

for (let i = 0; i <= N; i = i + 1) {
    flags = append(flags, 1);
};

flags[0] = 0;
flags[1] = 0;

for (let p = 2; p * p <= N; p = p + 1) {
    if (flags[p] == 1) {
        let m = p * p;
        while (m <= N) {
            flags[m] = 0;
            m = m + p;
        }
    }
}

printf("Primes: [ ");
for (let i = 2; i <= N; i = i + 1) {
    if (flags[i] == 1) {
        printf(str(i) + ", ");
    }
}
printf("]");